generator client {
  provider = "prisma-client-js"
  output = "../src/app/lib"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Province {
  id                 Int      @id @default(autoincrement())
  name               String
  capital            String
  slug               String
  language           String[]
  topCities          String[] @map("top_cities")
  immigrationRanking Int

  @@map("Provinces")
}
